<!-- This is to format the Excel like a table -->
<table border="1">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>Description</th>
		<th>Image Link</th>
		<!-- <th>Created at</th> -->
		<!-- <th>Updated at</th> -->
	</tr>
	<% @episodes.each do |episode| %>
		<tr>
			<td><%= episode.id %></td>
			<td><%= episode.name %></td>
			<td><%= episode.description %></td>
			<td><%= episode.image_url %></td>
			<!-- <td><%= episode.created_at %></td> -->
			<!-- <td><%= episode.updated_at %></td> -->
		</tr>
	<% end %>
</table>